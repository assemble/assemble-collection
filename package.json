{
  "name": "assemble-collection",
  "description": "Assemble plugin for creating paginated lists of files using file front matter.",
  "version": "0.1.2",
  "homepage": "https://github.com/assemble/assemble-collection",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "assemble/assemble-collection",
  "bugs": {
    "url": "https://github.com/assemble/assemble-collection/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "gulp-collection": "^0.3.0",
    "is-valid-app": "^0.2.0",
    "lazy-cache": "^2.0.1",
    "stream-combiner": "^0.2.2",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "assemble": "^0.16.2",
    "gulp-extname": "^0.2.2",
    "gulp-format-md": "^0.1.9",
    "mocha": "^3.0.0"
  },
  "keywords": [
    "assemble",
    "assemble-plugin",
    "collection",
    "gulp",
    "index",
    "indices",
    "list",
    "lists",
    "pager",
    "paginate",
    "pagination",
    "paginationator",
    "pipeline",
    "plugin"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "group-array",
        "gulp-collection",
        "paginationator",
        "permalinks",
        "placeholders"
      ]
    },
    "reflinks": [
      "paginationator",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
